<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp">
    <TextView
        android:id="@+id/label"
        android:layout_width="96.7dp"
        android:layout_height="46.9dp"
        android:text="Ticket number:"
        android:gravity="center_vertical"/>
    <EditText
        android:inputType="number"
        android:layout_width="179.3dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText1"
        android:layout_centerHorizontal="true"/>

    <Button
        android:id="@+id/ok"
        android:layout_width="89.5dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/entry"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="10dip"
        android:text="Next" />
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="362.5dp"
        android:layout_height="507.3dp"
        android:layout_centerInParent="true"
        android:id="@+id/relativeLayout1">
        <ImageView
            android:src="@drawable/firstclassseats"
            android:layout_width="358.9dp"
            android:layout_height="wrap_content"
            android:id="@+id/imageView1" />
        <ImageView
            android:src="@drawable/businessclassseats"
            android:layout_width="361.8dp"
            android:layout_height="wrap_content"
            android:id="@+id/imageView2" />

        </RelativeLayout>


</RelativeLayout>
